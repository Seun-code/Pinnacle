    padding: 10px;
    text-decoration: none;
    background-color: none;    
}
.latest {
    margin-top: 250px;
}
.down-con{
    display: flex;
    justify-content: space-evenly;
    align-items: center;
    list-style-type: none;
    text-align: left;
